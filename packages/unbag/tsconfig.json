{
  "compilerOptions": {
    "target": "esnext",
    "module": "node16",
    "moduleResolution": "node16",
    "allowJs": true,
    "emitDeclarationOnly": true,
    "declaration": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "noEmitOnError": false
  },
  "exclude": [
    "node_modules"
  ]
}